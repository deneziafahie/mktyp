   "source": [
    "# define the mph2fps function here\n",
    "def mph2fps(speed):\n",
    "    return speed*5280.0/3600\n",
    "\n",
    "print(\"MPH to FPS Calculator\")\n",
    "speed = float(input(\"Enter a speed in mph:\"))\n",
    "fps = mph2fps(speed)\n",
    "print(\"in 1 second you will go \"+str(fps)+\" feet\")"
   ]
